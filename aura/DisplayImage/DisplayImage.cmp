<aura:component implements="lightning:availableForFlowScreens" access="global" >
	<aura:attribute name="staticResourceName" type="String" default=""/>
    <aura:attribute name="width" type="String" default="80"/>
    <img class="singleimage" src="{!'/resource/' + v.staticResourceName}" style="{!'width:' + v.width + '%'}" /> 
</aura:component>