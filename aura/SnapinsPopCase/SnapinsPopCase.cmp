<aura:component implements="flexipage:availableForAllPageTypes,force:hasRecordId" access="global" controller="SnapinsPopCaseApex">

    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <lightning:workspaceAPI aura:id="workspace" />
    <lightning:utilityBarAPI aura:id="utilitybar" />

    <aura:attribute name="theCase" type="Case"/>
    <aura:attribute name="subject" type="String" default="Chat Session"/>
    <aura:attribute name="description" type="String" default=" "/>
    <aura:attribute name="recordTypeName" type="String" default="Chat Case"/>

</aura:component>